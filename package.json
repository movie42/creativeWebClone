{
  "name": "webGL",
  "version": "1.0.0",
  "scripts": {
    "server:build": "babel-node app.js",
    "server:development": "nodemon --exec babel-node app.js",
    "frontend:build": "webpack --progress --config webpack.config.build.js",
    "frontend:development": "webpack serve --progress --config webpack.config.development.js",
    "start": "concurrently --kill-others \"npm run frontend:development\" \"npm run server:development\""
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.10",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.7.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "mini-css-extract-plugin": "^2.4.6",
    "postcss-loader": "^6.2.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.46.0",
    "sass-loader": "^12.4.0",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@prismicio/client": "^6.1.0",
    "@prismicio/helpers": "^2.0.0",
    "concurrently": "^7.0.0",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.15",
    "pug": "^3.0.2"
  }
}
